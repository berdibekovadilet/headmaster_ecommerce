/InputGroupAddon>\r\n          <Input type=\"text\" name=\"image\" onChange={handleInputsValue} />\r\n        </InputGroup>\r\n        <br />\r\n        <form>\r\n          <ButtonGroup>\r\n            <Button\r\n              type=\"submit\"\r\n              className=\"btn btn-primary\"\r\n              onClick={() => addProduct(product)}\r\n            >\r\n              Save Product\r\n            </Button>\r\n            <Link to=\"/admin\">\r\n              <Button type=\"button\" className=\"btn btn-warning mx-3\">\r\n                Show Products\r\n              </Button>\r\n            </Link>\r\n          </ButtonGroup>\r\n          </form>\r\n      </div>\r\n    </Fragment>\r\n  );\r\n};\r\n\r\nexport default AddProduct;\r\n","D:\\Code\\Makers\\13week\\headmaster\\src\\container\\AdminPage\\EditProduct.jsx",[],"D:\\Code\\Makers\\13week\\headmaster\\src\\container\\Home\\SignIn\\SignIn.jsx",[],"D:\\Code\\Makers\\13week\\headmaster\\src\\container\\Home\\SignUp\\SignUp.jsx",[],"D:\\Code\\Makers\\13week\\headmaster\\src\\container\\Home\\ProductCard\\ProductCard.jsx",["130","131","132"],"import React, { useEffect, useContext } from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { Row, Col, Container, Button } from \"reactstrap\";\r\nimport { productsContext } from \"../../../contexts/ProductsContext\";\r\n\r\nconst ProductCard = (props) => {\r\n  const {\r\n    products,\r\n    addAndDeleteProductInCart,\r\n    getProductID,\r\n    productDetails,\r\n  } = useContext(productsContext);\r\n\r\n  useEffect(() => {\r\n    getProductID(props.match.params.id);\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"mainProductCard\">\r\n      {productDetails != null ? (\r\n        <>\r\n          <Container>\r\n            <Row>\r\n              <Col lg=\"4\" md=\"6\" xs=\"12\" style={{ marginleft: \"30px\" }}>\r\n                <img\r\n                  style={{ width: \"100%\" }}\r\n                  src={productDetails.image}\r\n           